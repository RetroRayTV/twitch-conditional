{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/twitchcraft",
  "version": "0.1.0",
  "title": "Twitchcraft",
  "category": "widgets",
  "icon": "twitch",
  "description": "Display conditional content if a Twitch Channel is live or not.",
  "supports": {
    "html": false
  },
  "attributes": {
    "userName": {
      "type": "string",
      "default": ""
    },
    "liveContent": {
      "type": "string",
      "default": 18
    },
    "offlineContent": {
      "type": "string",
      "default": 18
    }
  },
  "textdomain": "twitchcraft",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}